<?xml version="1.0" ?>
<robot name="my_robot" xmls:xacro="http://www.ros.org/wiki/xacro">
    <link name="link_chasis">
        <pose>0 0 0.1 0 0 0</pose>
        <inertial>
            <mass value="5"/>
            <origin rpy="0 0 0" xyz="0 0 0.1"/>
            <inertia ixx="0.3" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
        <collision name="col">
            <geometry>
                <box size="2 2 1"/>
            </geometry>
        </collision>

        <visual name="vis">
        <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="2 2 1"/>
            </geometry>
        </collision>
    </link>
</robot>